    "emergencyTitle": "Emergency Phrases",
    "emergency": [
      {
        "word": "Socorro!",
        "translation": "Help!",
        "explanation": "Urgent request for help",
        "example": "Socorro! Preciso de ajuda!",
        "exampleTranslation": "Help! I need help!"
      },
